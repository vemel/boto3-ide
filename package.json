{
	"name": "boto3-ide",
	"version": "0.2.1",
	"publisher": "Boto3typed",
	"displayName": "AWS boto3",
	"description": "Code auto-complete and type checking for all boto3 services",
	"license": "SEE LICENSE IN LICENSE",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/vemel/boto3-ide"
	},
	"bugs": {
		"url": "https://github.com/vemel/boto3-ide/issues",
		"email": "vlad.emelianov.nz@gmail.com"
	},
	"author": {
		"name": "Vlad Emelianov",
		"email": "vlad.emelianov.nz@gmail.com"
	},
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Programming Languages",
		"Linters",
		"Other"
	],
	"keywords": [
		"boto3",
		"botocore",
		"type checking",
		"auto-complete",
		"pylance"
	],
	"activationEvents": [
		"onCommand:boto3-ide.quickstart",
		"onCommand:boto3-ide.modify",
		"onCommand:boto3-ide.update",
		"onCommand:boto3-ide.docs",
		"onCommand:boto3-ide.autodiscover"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "boto3-ide.quickstart",
				"title": "AWS boto3: Quick Start"
			},
			{
				"command": "boto3-ide.modify",
				"title": "AWS boto3: Add or remove services"
			},
			{
				"command": "boto3-ide.update",
				"title": "AWS boto3: Update services"
			},
			{
				"command": "boto3-ide.docs",
				"title": "AWS boto3: Browse documentation"
			},
			{
				"command": "boto3-ide.autodiscover",
				"title": "AWS boto3: Auto-discover boto3 services in current project"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"build": "vsce package",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}