{
	"name": "boto3-ide",
	"displayName": "boto3-ide",
	"description": "AWS boto3 IDE",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:boto3-ide.modify",
		"onCommand:boto3-ide.update"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "boto3-ide.modify",
				"title": "AWS boto3 IDE: Add or remove boto3 services"
			},
			{
				"command": "boto3-ide.update",
				"title": "AWS boto3 IDE: Update installed services"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}